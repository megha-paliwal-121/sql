DESCRIBE customers
   
-- WE WRITE VARCHAR IN '' SINGLE QUOTE
-- AS CUSTOMER_ID IS AUTO INCREMENT SO WE WILL DEFINE IT AS DEFAULTS
INSERT INTO TABLE_NAME
